					["custom"] = "aura_env.region.configGroup = \"roll_the_bones\"\n\nlocal r = aura_env.region\nlocal b = r.bar\n\nif not b.bgIcon then\n    b.bgIcon = b:CreateTexture(nil, \"BACKGROUND\")\n    b.fgIcon = b:CreateTexture(nil, \"OVERLAY\")\n    b.mask = b:CreateMaskTexture()\n    \n    b.mask:SetTexture([[Interface\\AddOns\\WeakAuras\\Media\\Textures\\Square_FullWhite]], \"CLAMPTOBLACKADDITIVE\", \"CLAMPTOBLACKADDITIVE\", \"NEAREST\")\n    \n    b.mask:ClearAllPoints()\n    b.mask:SetPoint(\"TOPLEFT\", b.fgMask,\"TOPLEFT\", 1, 0)\n    b.mask:SetPoint(\"BOTTOMRIGHT\", b.fgMask,\"BOTTOMRIGHT\", 1, 0)\n    \n    b.bgIcon:SetSnapToPixelGrid(false)\n    b.bgIcon:SetTexelSnappingBias(0)\n    b.bgIcon:SetAllPoints(b.bg)\n    --b.bgIcon:SetTexCoord(GetTexCoord())\n    b.bgIcon:SetVertexColor(1,1,1,0.4)\n    b.bgIcon:SetDesaturated(true)\n    \n    b.fgIcon:SetSnapToPixelGrid(false)\n    b.fgIcon:SetTexelSnappingBias(0)\n    b.fgIcon:SetAllPoints(b.bg)\n    --b.fgIcon:SetTexCoord(GetTexCoord())\n    b.fgIcon:SetVertexColor(1,1,1,1)\n    b.fgIcon:AddMaskTexture(b.mask)\n    \n    b.parentRegion = r\n    b.originalUpdateProgress = b.UpdateProgress\n    \n    b.UpdateProgress = function(self)\n        self:originalUpdateProgress()\n        \n        local function GetTexCoord()\n            local currentCoord = { 0, 0, 0, 1, 1, 0, 1, 1 }\n            local texWidth = 1 - 0.5 * 0.3\n            local aspectRatio = 1\n            local r = self.parentRegion\n            \n            local width = r.width * math.abs(r.scalex);\n            local height = r.height * math.abs(r.scaley);\n            \n            if width == 0 or height == 0 then\n                aspectRatio = 1;\n            else\n                aspectRatio = width / height;\n            end\n            \n            local xRatio = aspectRatio < 1 and aspectRatio or 1\n            local yRatio = aspectRatio > 1 and 1 / aspectRatio or 1\n            \n            for i, coord in ipairs(currentCoord) do\n                local aspectRatio = (i % 2 == 1) and xRatio or yRatio\n                currentCoord[i] = (coord - 0.5) * texWidth * aspectRatio + 0.5\n            end\n            \n            return unpack(currentCoord)\n        end\n        \n        self.bgIcon:SetTexCoord(GetTexCoord())\n        self.fgIcon:SetTexCoord(GetTexCoord())\n    end\n    \n    b:UpdateProgress()\nend\n\nWeakAuras.SetTextureOrAtlas(b.bgIcon, r.displayIcon)\nWeakAuras.SetTextureOrAtlas(b.fgIcon, r.displayIcon)\n\nb.bg:SetAlpha(0)\nb.fg:SetAlpha(0)",
					["do_custom"] = true,
				},
