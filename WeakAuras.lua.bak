						["events"] = "NAME_PLATE_UNIT_ADDED, NAME_PLATE_UNIT_REMOVED, UNIT_AURA, PLAYER_TARGET_CHANGED",
						["custom"] = "function(allstates, event, unit)\n    local _, _, class = UnitClass(\"player\")\n    local _, _, raceID = UnitRace(\"player\")\n    if event == \"PLAYER_TARGET_CHANGED\" and aura_env.config[\"enableAlpha\"] then\n        for k,v in pairs(allstates) do\n            if not UnitIsUnit(v.unit, \"target\") and UnitExists(\"target\") then\n                v.changed = true\n                v.alpha = aura_env.config[\"alphaValue\"]\n            else\n                v.changed = true\n                v.alpha = 1\n            end\n        end\n    end\n    if event == \"NAME_PLATE_UNIT_ADDED\" or event == \"UNIT_AURA\" then\n        if UnitExists(unit)\n        and (\n            aura_env.config[\"specifyUnits\"][3]\n            and not UnitPlayerControlled(unit)\n            or aura_env.config[\"specifyUnits\"][2]\n            and UnitIsFriend(\"player\", unit)\n            or UnitPlayerControlled(unit)\n            and UnitIsEnemy(\"player\", unit)\n            and aura_env.config[\"specifyUnits\"][1]\n            and not UnitInRaid(unit)\n        )\n        and string.match(unit, \"nameplate\")\n        then\n            for _, state in pairs(allstates) do\n                if UnitIsUnit(state.unit, unit) then\n                    state.changed = true\n                    state.show = false\n                end\n            end\n            local count, name, icon, stacks, debuffType, duration, expirationTime, spellID = 0 \n            for i = 1, 255 do\n                name, icon, stacks, debuffType, duration, expirationTime, _,stealable,_, spellID = UnitBuff(unit, i) \n                if not name then break end\n                if ((aura_env.magicTypes[aura_env.config[\"specifyMagic\"]] == debuffType or aura_env.magicTypes[aura_env.config[\"specifyMagic\"]] ==  stealable) and aura_env.config[\"enableMagic\"] and (aura_env.config[\"magicClass\"][aura_env.magicclass[class]] or aura_env.config[\"magicClass\"][1] or raceID == 10 and aura_env.config[\"magicClass\"][8])) or (debuffType == \"\" and aura_env.config[\"enableEnrage\"] and (aura_env.config[\"enrageClass\"][aura_env.enrageclass[class]] or aura_env.config[\"enrageClass\"][1]))\n                then\n                    allstates[unit..count] = {\n                        changed = true,\n                        show = true,\n                        stacks = stacks,\n                        icon = icon,\n                        progressType = \"timed\",\n                        duration = duration,\n                        expirationTime = expirationTime,\n                        unit = unit,\n                        offset = count*aura_env.region.width,\n                        autoHide = true,\n                        stealable = stealable,\n                        debuffType = debuffType,\n                    }\n                    count = count+1\n                end\n            end\n        end\n    end\n    if event == \"NAME_PLATE_UNIT_REMOVED\" then\n        for _, state in pairs(allstates) do\n            if state.unit == unit then\n                state.show = false\n                state.changed = true\n            end\n        end\n    end\n    return true\nend\n\n-- big thanks to asakawa for major help",
