				["init"] = {
					["custom"] = "aura_env.dice_ids = {\n    [193356] = true, -- Broadside\n    [193357] = true, -- Ruthless Precision\n    [193358] = true, -- Grand Melee\n    [193359] = true, -- True Bearing\n    [199600] = true, -- Buried Treasure\n    [199603] = true, -- Skull and Crossbones\n}\n\naura_env.checkDice = function(event, unit)\n    if not unit == \"player\" then\n        return\n    end\n    \n    local foundDices = {}\n    local count = 0\n    \n    for i = 1, 255 do\n        local name, _, _, _, _, _, _, _, _, spellId = UnitAura(\"player\", i, \"HELPFUL\")\n        if not name then break end\n        \n        if aura_env.dice_ids[spellId] then\n            count = count + 1\n            foundDices[spellId] = true\n        end\n    end\n    \n    -- Loaded Dice buff ID: 256171\n    if count < 2 and not foundDices[256171] then\n        local cfg = aura_env.config.resources.roll_the_bones\n        \n        if cfg[\"keep-broadside\"] and foundDices[193356] then\n            return false\n        elseif cfg[\"keep-ruthless-precision\"] and foundDices[193357] then\n            return false\n        elseif cfg[\"keep-grand-melee\"] and foundDices[193358] then\n            return false\n        elseif cfg[\"keep-true-bearing\"] and foundDices[193359] then\n            return false\n        elseif cfg[\"keep-buried-treasure\"] and foundDices[199600] then\n            return false\n        elseif cfg[\"keep-skull-and-crossbones\"] and foundDices[199603] then\n            return false\n        end\n    end\n    \n    if count >= 2 then\n        return false\n    else\n        return true\n    end\nend",
