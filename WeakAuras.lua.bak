					["custom"] = "local k = aura_env.cloneId\nlocal v = aura_env.state\n\n\nlocal Region = WeakAuras.GetRegion(aura_env.id, k)\nlocal w=Region:GetWidth()\n\nif k>=1 then            \n    Region:ClearAllPoints()\n    Region:SetPoint(\"CENTER\", WeakAuras.GetRegion(aura_env.id, k-1), \"CENTER\", 0,-w)\nend\n\n\n\n\n\n\n",
